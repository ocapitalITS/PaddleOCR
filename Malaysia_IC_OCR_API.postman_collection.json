{
  "info": {
    "_postman_id": "malaysia-ic-ocr-api",
    "name": "Malaysia IC OCR API",
    "description": "API for Malaysia Identity Card (MyKad) OCR Processing",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:5000/api/health",
          "protocol": "http",
          "host": ["localhost"],
          "port": "5000",
          "path": ["api", "health"]
        },
        "description": "Check if the API is running"
      },
      "response": []
    },
    {
      "name": "API Info",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:5000/api/info",
          "protocol": "http",
          "host": ["localhost"],
          "port": "5000",
          "path": ["api", "info"]
        },
        "description": "Get API information and available endpoints"
      },
      "response": []
    },
    {
      "name": "Process Single IC Image",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": ""
            }
          ]
        },
        "url": {
          "raw": "http://localhost:5000/api/ocr",
          "protocol": "http",
          "host": ["localhost"],
          "port": "5000",
          "path": ["api", "ocr"]
        },
        "description": "Upload a Malaysia IC image (JPG/PNG) or PDF and get OCR results\n\nResponse includes:\n- IC Number\n- Name\n- Gender\n- Religion\n- Address\n- Postcode Validation\n- Document Type\n- Orientation Angle\n- Raw OCR text"
      },
      "response": []
    },
    {
      "name": "Batch Process Multiple Images",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "files",
              "type": "file",
              "src": []
            }
          ]
        },
        "url": {
          "raw": "http://localhost:5000/api/ocr/batch",
          "protocol": "http",
          "host": ["localhost"],
          "port": "5000",
          "path": ["api", "ocr", "batch"]
        },
        "description": "Upload multiple Malaysia IC images and get results for all of them\n\nUsage:\n1. Select multiple files in the file upload\n2. All files will be processed in one request\n3. Response includes results for each file"
      },
      "response": []
    }
  ]
}
